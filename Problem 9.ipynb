    "def solution():\n",
    "    for a in range(1,999):\n",
    "        for b in range(1,999-a):\n",
    "            c = 1000 - a - b \n",
    "            if a*a + b*b == c*c:\n",
    "                return a*b*c\n",
    "            \n",
    "solution()"
